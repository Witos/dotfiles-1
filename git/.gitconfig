[user]
	name = aouelete 
	email = aouellette2016@gmail.com

[credential]
	helper = cache --timeout=3600

[push]
	default = simple

[color]
    ui = auto

[help]
    autocorrect = 10

[alias]
    graph = log --graph --color
    st = status
    com = commit
    chout = checkout
    unstage = reset HEAD --
    last = log -1 HEAD

[sendemail]
    smtpEncryption = tls
    smtpServer = smtp.gmail.com
    smtpUser = aouellette2016@gmail.com
    smtpServerPort = 587

[merge]
    tool = vimdiff2
